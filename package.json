{
  "name": "autonomous-vibe-coding-agent",
  "version": "1.0.0",
  "description": "An autonomous vibe coding agent that transforms natural language into code using AI",
  "main": "src/app.js",
  "scripts": {
    "start": "node --max-old-space-size=512 --expose-gc src/app.js",
    "dev": "nodemon --max-old-space-size=512 --expose-gc src/app.js",
    "deploy": "./deploy.sh",
    "build:all": "cd coder1-ide-next && npm run build",
    "verify:deployment": "curl -f http://localhost:3000/ide || echo 'Deployment verification failed'",
    "rollback": "if [ -f .last_backup ]; then BACKUP_DIR=$(cat .last_backup); rm -rf public/ide && mv \"$BACKUP_DIR\" public/ide && echo 'Rollback completed'; else echo 'No backup found'; fi",
    "terminal-server": "node terminal-server.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "test:claude-cli": "node test-claude-cli.js",
    "install:claude-cli": "npm install -g claude-code",
    "rules:setup": "node setup-github-rules.js",
    "rules:refresh": "curl -X POST http://localhost:3000/api/agent/rules/refresh",
    "rules:status": "curl -s http://localhost:3000/api/agent/rules/status | jq",
    "rules:validate": "node -e \"const config = require('./src/config/rules-config'); console.log('Validation:', config.validateConfig());\"",
    "preload": "curl -X POST http://localhost:3000/api/repository-admin/preload/start -H 'Content-Type: application/json'",
    "preload:test": "TEST_PRELOAD=true npm start",
    "preload:skip": "SKIP_PRELOAD=true npm start",
    "preload:status": "curl -s http://localhost:3000/api/repository-admin/preload/status | jq",
    "preload:stop": "curl -X POST http://localhost:3000/api/repository-admin/preload/stop",
    "preload:list": "curl -s http://localhost:3000/api/repository-admin/preload/list | jq",
    "monitor:terminal": "./scripts/terminal-monitor.sh check",
    "monitor:recovery": "./scripts/terminal-monitor.sh recovery",
    "monitor:report": "./scripts/terminal-monitor.sh report",
    "monitor:watch": "./scripts/terminal-monitor.sh monitor"
  },
  "keywords": [
    "ai",
    "coding",
    "autonomous",
    "agent",
    "express",
    "nodejs"
  ],
  "author": "Your Name",
  "license": "MIT",
  "dependencies": {
    "@21st-dev/cli": "^0.0.29",
    "@21st-dev/magic": "^0.1.0",
    "@airtop/sdk": "^0.1.44",
    "@anthropic-ai/sdk": "^0.59.0",
    "@babel/parser": "^7.28.3",
    "@babel/traverse": "^7.28.3",
    "@xenova/transformers": "^2.17.2",
    "@xterm/addon-fit": "^0.10.0",
    "@xterm/xterm": "^5.5.0",
    "archiver": "^5.3.1",
    "axios": "^1.10.0",
    "bcryptjs": "^3.0.2",
    "cheerio": "^1.0.0-rc.12",
    "chromadb": "^3.0.14",
    "cors": "^2.8.5",
    "crypto-js": "^4.2.0",
    "dotenv": "^17.0.1",
    "express": "^4.18.2",
    "express-session": "^1.18.2",
    "express-validator": "^7.2.1",
    "form-data": "^4.0.4",
    "glob": "^11.0.3",
    "http-proxy": "^1.18.1",
    "http-proxy-middleware": "^3.0.5",
    "js-yaml": "^4.1.0",
    "jsonwebtoken": "^9.0.2",
    "mammoth": "^1.10.0",
    "mime-types": "^2.1.35",
    "multer": "^2.0.2",
    "node-fetch": "^2.6.12",
    "node-pty": "^0.10.1",
    "nodemailer": "^7.0.5",
    "openai": "^5.10.1",
    "pdf-parse": "^1.1.1",
    "session-file-store": "^1.5.0",
    "sharp": "^0.33.0",
    "socket.io": "^4.8.1",
    "socket.io-client": "^4.8.1",
    "sqlite3": "^5.1.7",
    "uuid": "^11.1.0",
    "wired-elements": "^2.1.0",
    "ws": "^8.18.3"
  },
  "devDependencies": {
    "@playwright/test": "^1.55.0",
    "eslint": "^9.34.0",
    "nodemon": "^3.1.10"
  },
  "engines": {
    "node": ">=14.0.0"
  }
}
