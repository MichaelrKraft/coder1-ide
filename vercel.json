{
  "version": 2,
  "builds": [
    {
      "src": "src/app.js",
      "use": "@vercel/node"
    },
    {
      "src": "coder1-ide/ide-build/**",
      "use": "@vercel/static"
    },
    {
      "src": "public/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/src/app.js"
    },
    {
      "src": "/socket.io/(.*)",
      "dest": "/src/app.js"
    },
    {
      "src": "/ide/static/(.*)",
      "dest": "/coder1-ide/ide-build/static/$1"
    },
    {
      "src": "/ide/(.*\\.js|.*\\.css|.*\\.png|.*\\.jpg|.*\\.ico|.*\\.json)",
      "dest": "/coder1-ide/ide-build/$1"
    },
    {
      "src": "/ide$",
      "dest": "/src/app.js"
    },
    {
      "src": "/ide/",
      "dest": "/src/app.js"
    },
    {
      "src": "/(.*)",
      "dest": "/public/$1"
    }
  ],
  "env": {
    "NODE_ENV": "production",
    "VERCEL": "1"
  }
}